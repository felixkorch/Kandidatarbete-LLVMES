LDA %2, Y
BEQ exit
STA $2008
INY
JMP %1
#endmacro

#macro TAX_CMP 2
LDA %1
TAX 
CPX %2
#endmacro

.org $8000

ok_str:
.db "ok", 10, 0

err_str:
.db "err", 10, 0

; Flags: N V U B D I Z C

; Immediate test
main:
LDY #0

LDX #$02
TXS
TSX
CPX #$02
BNE fail

LDX #$F1
TXS
TSX
CPX #$F1
BNE fail

LDX #$A3
TXS
TSX
CPX #$A3
BNE fail


JMP success


fail:
LDA #1
JMP exit

success:
LDA #0
JMP exit

exit:
STA $200F

.org $FFFC
.dw main ; Reset routine