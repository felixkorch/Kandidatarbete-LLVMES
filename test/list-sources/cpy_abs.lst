.org $8000

#macro PRINT_NCZ 0
STA $200C
STA $200D
STA $200E
#endmacro

#macro LOAD_Y_CMP 1
LDY %1
STA $200B
CPY $2000
#endmacro

main:
LDA #$0A
STA $2009 ; should print 10
STA $2000 ; sta reg_a (=0x0A) to 0x2000 in mem

LOAD_Y_CMP #$0A
