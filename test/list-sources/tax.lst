#macro PRINT 2
%1:
LDA %2, X
BEQ exit
STA $2008
INX
JMP %1
#endmacro

#macro TAX_CMP 3
LDA %1
TAX %2
CPX %3
#endmacro

.org $8000

ok_str:
.db "ok", 10, 0

err_str:
.db "err", 10, 0

; Flags: N V U B D I Z C

; Immediate test
main:


TAX_CMP #$05 #$05 #$05
BNE print_err



JMP print_ok


PRINT print_ok ok_str
PRINT print_err err_str

exit:
LDA #0
STA $200F ; exit 0

.org $FFFC
.dw main ; Reset routine